# Configuração do Banco de Dados PostgreSQL
# Formato: postgres://usuario:senha@host:porta/database?sslmode=require
DATABASE_URL=postgres://user:password@host:5432/database?sslmode=require

# Chave secreta para assinar tokens JWT
# Em produção, use uma string aleatória forte (ex: openssl rand -base64 32)
JWT_SECRET=seu_secret_super_secreto_troque_em_producao

# Porta onde o servidor irá rodar (opcional, padrão: 8080)
PORT=8080

